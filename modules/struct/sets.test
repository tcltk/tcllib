} [tcltest::wrongNumArgs {::struct::set::Ssubtract} {Avar B} 0]

test set-13.1 {subtract} {
    catch {setop subtract A} msg
    set msg
} [tcltest::wrongNumArgs {::struct::set::Ssubtract} {Avar B} 1]

test set-13.2 {subtract} {
    catch {unset A}
    catch {setop subtract A B} msg
